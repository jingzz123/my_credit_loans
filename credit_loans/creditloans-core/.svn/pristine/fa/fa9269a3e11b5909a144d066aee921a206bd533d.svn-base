<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="cn.creditloans.core.dao.af.MetadataFieldDao">

	<resultMap id="resultMap" type="cn.creditloans.core.entity.af.MetadataField">
        <id property="id" column="id" />
		<result property="schemaId" column="schema_id" />
		<result property="name" column="name" />
		<result property="description" column="description" />
		<result property="type" column="type" />
		<result property="createUserId" column="create_user_id" />
		<result property="createTime" column="create_time" />
		<result property="upadteUserId" column="upadte_user_id" />
		<result property="updateTime" column="update_time" />
    </resultMap>
    
    <!-- 插入单个 -->
    <insert id="insert" parameterType="cn.creditloans.core.entity.af.MetadataField">
    	<selectKey order="AFTER" keyProperty="id" resultType="java.lang.Integer">
		    SELECT currval('af_metadata_field_id_seq')
		</selectKey>
 		INSERT INTO af_metadata_field (schema_id,name,description,type,create_user_id,
 			create_time,upadte_user_id,update_time)
		VALUES (#{schemaId}, #{name}, #{description}, #{type}, #{createUserId},
			now(), #{upadteUserId}, now());
    </insert>
    
    <!-- 修改单个 -->
    <update id="update"  parameterType="cn.creditloans.core.entity.af.MetadataField">
    	update af_metadata_field set name = #{name}, description = #{description}, 
    	            type = #{type}, upadte_user_id = #{upadteUserId},
					update_time = now() where id = #{id}
    </update>
    
    <!-- 删除单个 -->
    <delete id="delete" parameterType="int">
    	delete from af_metadata_field where id = #{id} 
    </delete>
    
    <!-- 查询单个 -->
    <select id="select" resultMap="resultMap" parameterType="int">
    	select id,schema_id,name,description,type,create_user_id,create_time,
    		upadte_user_id,update_time from af_metadata_field where id = #{id} 
    </select>
    
    <!-- 查询所有 -->
    <select id="selectAll" resultMap="resultMap" parameterType="int">
    	select id,schema_id,name,description,type,create_user_id,create_time,
    		upadte_user_id,update_time from af_metadata_field 
    </select>
    
	<!-- 查询总量，这里的需要根据实际条件进行查询 -->
	<select id="selectPageCount" resultType="int" parameterType="cn.creditloans.core.entity.af.MetadataField">
        select count(1) FROM af_metadata_field where 1=1 and schema_id = #{schemaId}
        <if test="name != null and '' != name">  
			and name like '%${name}%'  
		</if>
    	<if test="description != null and '' != description">  
			and description like '%${description}%'  
		</if>
    </select>
    
    <!-- 根据实际情况需要修改语句 -->
    <select id="selectPageList" resultMap="resultMap">
         select id,schema_id,name,description,type,create_user_id,create_time,
    		upadte_user_id,update_time from af_metadata_field where 1=1 and schema_id = #{metadataField.schemaId}
    	<if test="metadataField.name != null and '' != metadataField.name">  
			and name like '%${metadataField.name}%'  
		</if>
    	<if test="metadataField.description != null and '' != metadataField.description">  
			and description like '%${metadataField.description}%'  
		</if>
         limit #{pm.pageSize} offset #{pm.startCount}
    </select>

</mapper>