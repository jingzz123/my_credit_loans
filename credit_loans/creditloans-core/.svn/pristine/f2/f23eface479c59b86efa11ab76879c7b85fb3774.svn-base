<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="cn.creditloans.core.dao.af.BaseVariableTransactionDao">

	<resultMap id="resultMap" type="cn.creditloans.core.entity.af.BaseVariableTransaction">
        <id property="id" column="id" />
		<result property="name" column="name" />
		<result property="description" column="description" />
		<result property="fieldId" column="field_id" />
		<result property="createUserId" column="create_user_id" />
		<result property="createTime" column="create_time" />
		<result property="upadteUserId" column="upadte_user_id" />
		<result property="updateTime" column="update_time" />
    </resultMap>
    
    <!-- 插入单个 -->
    <insert id="insert" parameterType="cn.creditloans.core.entity.af.BaseVariableTransaction">
    	<selectKey order="AFTER" keyProperty="id" resultType="java.lang.Integer">
		    SELECT currval('af_base_variable_transaction_id_seq')
		</selectKey>
 		INSERT INTO af_base_variable_transaction (name,description,field_id,
 			create_user_id,create_time,upadte_user_id,update_time)
		VALUES (#{name}, #{description}, #{fieldId},  
			#{createUserId}, now(), #{upadteUserId}, now());
    </insert>
    
    <!-- 修改单个 -->
    <update id="update"  parameterType="cn.creditloans.core.entity.af.BaseVariableTransaction">
    	update af_base_variable_transaction set name = #{name}, description = #{description}, 
    	            field_id = #{fieldId}, update_time = now() where id = #{id}
    </update>
    
    <!-- 删除单个 -->
    <delete id="delete" parameterType="int">
    	delete from af_base_variable_transaction where id = #{id} 
    </delete>
    <resultMap id="relationRM" type="cn.creditloans.core.entity.af.BaseVariableTransaction" extends="resultMap">
        <collection property="baseVariableRelationList" javaType="ArrayList" column="id" ofType="cn.creditloans.core.entity.af.BaseVariableRelation" 
                    select="selectBaseVariableRelationListByVariableId"/>           
    </resultMap>
    <resultMap id="baseVariableRelationResultMap" type="cn.creditloans.core.entity.af.BaseVariableRelation">
         <id property="id" column="id" />
		<result property="baseId" column="base_id" />
		<result property="type" column="type" />
		<result property="variableId" column="variable_id" />
		<result property="sequence" column="sequence" />
		<result property="createUserId" column="create_user_id" />
		<result property="createTime" column="create_time" />
		<result property="upadteUserId" column="upadte_user_id" />
		<result property="updateTime" column="update_time" />
    </resultMap>
    <select id="selectBaseVariableRelationListByVariableId" parameterType="int" resultMap="baseVariableRelationResultMap">
    	select * from af_base_element_variable_relation where variable_id = #{id}
    </select>
    <!-- 查询单个 -->
    <select id="select" resultMap="relationRM" parameterType="int">
    	select * from af_base_variable_transaction where id = #{id} 
    </select>
    
    <!-- 查询所有 -->
    <select id="selectAll" resultMap="resultMap" parameterType="int">
    	select * from af_base_variable_transaction
    </select>
    
	<!-- 查询总量，这里的需要根据实际条件进行查询 -->
	<select id="selectPageCount" resultType="int" parameterType="cn.creditloans.core.entity.af.BaseVariableTransaction">
        select count(1) FROM af_base_variable_transaction where 1=1
        <if test="name != null and '' != name">  
			and name like '%${name}%'  
		</if>
		<if test="description != null and '' != description">
			and description like '%${description}%' 
		</if>
    </select>
    
    <!-- 根据实际情况需要修改语句 -->
    <select id="selectPageList" resultMap="resultMap">
         select * from af_base_variable_transaction where 1=1
          <if test="entity.name != null and '' != entity.name">  
			and name like '%${entity.name}%'  
		</if>
		<if test="entity.description != null and '' != entity.description">
			and description like '%${entity.description}%' 
		</if>
         limit #{pm.pageSize} offset #{pm.startCount}
    </select>
<!-- 批量插入关系 -->
    <insert id="batchInsertBaseVariableRelation" parameterType="java.util.List">
        insert into af_base_element_variable_relation (base_id, type, variable_id, sequence, create_user_id, create_time, upadte_user_id,update_time)  
        VALUES  
        <foreach collection="list" item="item" index="index" separator=",">  
            (#{item.baseId}, #{item.type}, #{item.variableId},  #{item.sequence}, #{item.createUserId}, now(), #{item.upadteUserId}, now())
        </foreach>  
    </insert>
    <!-- 查询最大序列 -->
    <select id="getMaxSequence" parameterType="map" resultType="Integer">
    	select max(${sequence}) from af_base_element_variable_relation where base_id = #{baseId} and type = #{type}
    </select>
    
    <delete id="deleteBaseVariableRelationByVariable" parameterType="map">
    	delete from af_base_element_variable_relation where variable_id = #{variableId} and type= #{type}
    </delete>
    
    <!-- 检查名字是否唯一 -->
    <select id="selectNameIsExit" resultType="int" parameterType="string">
    	select count(1)  from af_base_variable_transaction where name = #{name}
    </select>
</mapper>