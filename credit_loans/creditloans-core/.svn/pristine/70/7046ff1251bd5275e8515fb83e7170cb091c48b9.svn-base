		<field name="work_tel" type="Text" desc="单位电话"/>
		<field name="family_address" type="Text" desc="家庭地址"/>
		<field name="family_tel" type="Text" desc="家庭电话"/>
		<field name="applied_on" type="Text" desc="申请日期"/>
		<property>
			<delimiter>,</delimiter>
		</property>
	</schema>

</schemadef>