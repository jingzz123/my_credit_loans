		<property>
			<delimiter>,</delimiter>
		</property>
	</schema>

	<!--套现持卡人信息-->
	<schema name="CashCardholderInfo" type="Various_length">
		<field name="uuid" type="Text" desc="唯一码"/>
		<field name="bank_id" type="Text" desc="通报行"/><!--通报行：与“银行代码表”中“名称”字段值一致，同时在一定范围内-->
		<field name="validate_status" type="Text" desc="信息核实状态"/><!--信息核实状态：可疑、确认-->
		<field name="name" type="Text" desc="姓名"/>
		<field name="id_type" type="Text" desc="证件类型"/><!--证件类型：身份证、……-->
		<field name="id_number" type="Text" desc="证件号码"/><!--证件号码：如[证件类型]=身份证，则符合身份证号码验证规则-->
		<field name="credit_quota" type="Text" desc="信用额度"/>
		<field name="involved_money" type="Text" desc="涉案金额"/>
		<field name="merchant_name" type="Text" desc="套现涉及商户名称"/>
		<field name="merchant_no" type="Text" desc="套现涉及商户代码"/>
		<field name="work_name" type="Text" desc="单位名称"/>
		<field name="work_tel" type="Text" desc="单位电话"/>
		<field name="phone" type="Text" desc="手机"/>
		<field name="produced_on" type="Text" desc="确认日期"/><!--确认日期：yyyyMMdd,空值为“”-->
		<field name="comments" type="Text" desc="备注"/>
		<property>
			<delimiter>,</delimiter>
		</property>
	</schema>

	<!--套现商户信息-->
	<schema name="CashTenantInfo" type="Various_length">
		<field name="uuid" type="Text" desc="唯一码"/><!---->
		<field name="bank_id" type="Text" desc="通报行"/><!--通报行：与“银行代码表”中“名称”字段值一致，同时在一定范围内-->
