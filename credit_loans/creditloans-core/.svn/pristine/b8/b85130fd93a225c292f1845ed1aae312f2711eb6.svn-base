package cn.creditloans.core.dto.af;

import java.io.Serializable;
import java.util.Date;

/**
 * af_base_element
 * 
 * @author Ash
 * 
 */
public class BaseElementDTO implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 3523216485442723442L;
	private int id;
	private String name; // 名称
	private String description; // 描述
	private String isInner; // Y表示是内部创建
	private int departmentId; // 机构ID
	private int schemaId; //shcemaID
	private String isUsed = "Y"; // Y表示是有用
	private int createUserId; // 创建用户
	private Date createTime; // 创建时间
	private int upadteUserId; // 最后修改用户
	private Date updateTime; // 最后修改时间

	private int actionNameFlag;
	private int actionVariableFlag;
	private int variableSimpleFlag;
	private int variableTransactionFlag;
	private int variableSituationFlag;
	private int variableExpressionFlag;
	
	public int getVariableSimpleFlag() {
		return variableSimpleFlag;
	}

	public void setVariableSimpleFlag(int variableSimpleFlag) {
		this.variableSimpleFlag = variableSimpleFlag;
	}

	public int getVariableTransactionFlag() {
		return variableTransactionFlag;
	}

	public void setVariableTransactionFlag(int variableTransactionFlag) {
		this.variableTransactionFlag = variableTransactionFlag;
	}

	public int getVariableSituationFlag() {
		return variableSituationFlag;
	}

	public void setVariableSituationFlag(int variableSituationFlag) {
		this.variableSituationFlag = variableSituationFlag;
	}

	public int getVariableExpressionFlag() {
		return variableExpressionFlag;
	}

	public void setVariableExpressionFlag(int variableExpressionFlag) {
		this.variableExpressionFlag = variableExpressionFlag;
	}

	public int getActionNameFlag() {
		return actionNameFlag;
	}

	public void setActionNameFlag(int actionNameFlag) {
		this.actionNameFlag = actionNameFlag;
	}

	public int getActionVariableFlag() {
		return actionVariableFlag;
	}

	public void setActionVariableFlag(int actionVariableFlag) {
		this.actionVariableFlag = actionVariableFlag;
	}

	public int getSchemaId() {
		return schemaId;
	}

	public void setSchemaId(int schemaId) {
		this.schemaId = schemaId;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getIsInner() {
		return isInner;
	}

	public void setIsInner(String isInner) {
		this.isInner = isInner;
	}

	public int getDepartmentId() {
		return departmentId;
	}

	public void setDepartmentId(int departmentId) {
		this.departmentId = departmentId;
	}

	public String getIsUsed() {
		return isUsed;
	}

	public void setIsUsed(String isUsed) {
		this.isUsed = isUsed;
	}

