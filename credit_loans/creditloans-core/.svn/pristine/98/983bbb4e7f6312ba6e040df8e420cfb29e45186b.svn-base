package cn.creditloans.core.entity.platform;

import java.util.ArrayList;
import java.util.Date;
import java.util.List;

/**
 * 平台角色信息实体
 * @author Austin
 *
 */
public class PlatformRole {
	
	private int id;
	
	private String name;
	
	private String description;
	
	private int status;
	
	private Date createTime;
	
	private Date updateTime;

	private List<PlatformRoleMenu> platformRoleMenuList = new ArrayList<PlatformRoleMenu>();
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Date getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

	public List<PlatformRoleMenu> getPlatformRoleMenuList() {
		return platformRoleMenuList;
	}

	public void setPlatformRoleMenuList(List<PlatformRoleMenu> platformRoleMenuList) {
		this.platformRoleMenuList = platformRoleMenuList;
	}
}
