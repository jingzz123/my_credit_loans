<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="cn.creditloans.core.dao.af.MetadataRelationDao">

	<resultMap id="resultMap" type="cn.creditloans.core.entity.af.MetadataRelation">
        <id property="id" column="id" />
		<result property="mainSchemaId" column="main_schema_id" />
		<result property="childSchemaId" column="child_schema_id" />
		<result property="sequence" column="sequence" />
		<result property="createUserId" column="create_user_id" />
		<result property="createTime" column="create_time" />
		<result property="upadteUserId" column="upadte_user_id" />
		<result property="updateTime" column="update_time" />
    </resultMap>
    
    <!-- 插入单个 -->
    <insert id="insert" parameterType="cn.creditloans.core.entity.af.MetadataRelation">
    	<selectKey order="AFTER" keyProperty="id" resultType="java.lang.Integer">
		    SELECT currval('af_metadata_nest_relation_id_seq')
		</selectKey>
 		INSERT INTO af_metadata_nest_relation (main_schema_id,child_schema_id,sequence,
 			create_user_id,create_time,upadte_user_id,update_time)
		VALUES (#{mainSchemaId}, #{childSchemaId}, #{sequence},
			#{createUserId}, now(), #{upadteUserId}, now());
    </insert>
    
    <!-- 修改单个 -->
    <update id="update"  parameterType="cn.creditloans.core.entity.af.MetadataRelation">
    	update af_metadata_nest_relation set child_schema_id = #{childSchemaId}, 
    	            sequence = #{sequence}, upadte_user_id = #{upadteUserId},
					update_time = now() where id = #{id}
    </update>
    
    <!-- 删除单个 -->
    <delete id="delete" parameterType="int">
    	delete from af_metadata_nest_relation where id = #{id} 
    </delete>
    
    <!-- 查询单个 -->
    <select id="select" resultMap="resultMap" parameterType="int">
    	select * from af_metadata_nest_relation where id = #{id} 
    </select>
    
    <!-- 查询所有 -->
    <select id="selectAll" resultMap="resultMap" parameterType="int">
    	select * from af_metadata_nest_relation 
    </select>
    
	<!-- 查询总量，这里的需要根据实际条件进行查询 -->
	<select id="selectPageCount" resultType="int" parameterType="cn.creditloans.core.entity.af.MetadataRelation">
        select count(1) FROM af_metadata_nest_relation
    </select>
    
   <resultMap type="cn.creditloans.core.entity.af.MetadataRelation" id="selectRelationAndSchemaRM" extends="resultMap">
  		<association property="metadataSchema" javaType="cn.creditloans.core.entity.af.MetadataSchema" column="main_schema_id" select="selectMetadataSchemaById"/>
  </resultMap>
  
  <resultMap type="cn.creditloans.core.entity.af.MetadataSchema" id="metadataSchemaResultMap">
  			<id property="id" column="id" />
			<result property="schemaId" column="schema_id" />
			<result property="name" column="name" />
			<result property="description" column="description" />
			<result property="type" column="type" />
			<result property="delimiter" column="delimiter" />
			<result property="isNested" column="is_nested" />
			<result property="idLength" column="id_length" />
			<result property="isInner" column="is_inner" />
			<result property="departmentId" column="department_id" />
			<result property="isUsed" column="is_used" />
			<result property="createUserId" column="create_use r_id" />
			<result property="createTime" column="create_time" />
			<result property="upadteUserId" column="upadte_user_id" />
			<result property="updateTime" column="update_time" />
  </resultMap>
    <select id="selectMetadataSchemaById" parameterType="int" resultMap="metadataSchemaResultMap">
        select id, user_id, role_id from platform_user_role where user_id = #{id}
    </select>
    <!-- 根据实际情况需要修改语句 -->
    <select id="selectPageList" resultMap="resultMap">
         select * from af_metadata_nest_relation where main_schema_id = #{entity.mainSchemaId}
         <if test="entity.childSchemaId != 0">
          and child_schema_id = #{entity.childSchemaId}
         </if>
         <if test="entity.sequence != 0">
          and sequence = #{entity.sequence}
         </if>
         limit #{pm.pageSize} offset #{pm.startCount}
    </select>

	<select id="selectByMainSchemaId" resultMap="resultMap" parameterType="int">
         select * from af_metadata_nest_relation where main_schema_id = #{mainSchemaId}
    </select>
    
    <!-- 查询sequence是否已存在 -->
    <select id="selectSequence" resultType="int" parameterType="map">
    	select count(1) from af_metadata_nest_relation where sequence = #{sequence}  and main_schema_id = #{mainSchemaId}
    </select>
    
     
    <!-- 查询schema名称是否已存在 -->
    <select id="selectChildSchemaName" resultType="int" parameterType="map">
    	select count(1) from af_metadata_nest_relation where  main_schema_id = #{mainSchemaId} and child_schema_id = #{childSchemaId}
    </select>
     <!-- 根据main_schema_id child_schema_id查询单个 -->
    <select id="selectByIds" resultMap="resultMap" parameterType="map">
    	select * from af_metadata_nest_relation where  main_schema_id = #{mainSchemaId} and child_schema_id = #{childSchemaId}
    </select>
     <!-- 根据main_schema_id child_schema_id删除单个 -->
    <delete id="deleteByIds" parameterType="map">
    	delete from af_metadata_nest_relation where main_schema_id = #{mainSchemaId} and child_schema_id = #{childSchemaId}
    </delete>
</mapper>